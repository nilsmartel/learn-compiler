function ifelse(b, n) {
    let x = 0
    if b == true {
        let something = 7
        something += 1
        x += something
    } else {
        x += n
    }

    return x
}

funciton ifelse
b
n
x
jumpif *a branch1
jump branch2
branch1:
    something
    store something, 7
    add something, *something, 1
    add x, *x, something
    jump branch3
branch2:
    add x, *x, n
    jump branch3
branch3:
    return x
